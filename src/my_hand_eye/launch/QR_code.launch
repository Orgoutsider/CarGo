<?xml version="1.0"?>
<launch>

    <node pkg="nodelet" type="nodelet" name="my_hand_eye_manager"  args="manager"/>

    <!-- Example camera launch -->
    <include file="$(find my_hand_eye)/launch/eye.launch" />


    <node pkg="nodelet" type="nodelet" name="my_hand_eye_QR_code" args="load my_hand_eye/QRcodeDetector my_hand_eye_manager" output="screen"></node>

    <node pkg="zbar_ros" type="barcode_reader_node" name="barcode_reader" output="screen">
        <param name="throttle_repeated_barcodes" type="double" value="2.0"/>
        <remap from="image" to="/usb_cam/image_raw" />
    </node>

</launch>
