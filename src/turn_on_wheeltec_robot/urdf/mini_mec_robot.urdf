            <!-- 5.878 5.850 5.794 -->
            <origin xyz="0 0 0.06" rpy="0 0 0" />
            <geometry>
                <box size="0.266 0.1517 0.0033"/>
            </geometry>
            <material name="white">
                <color rgba="1 1 1 1"/>
            </material>
        </visual>
    </link>   

